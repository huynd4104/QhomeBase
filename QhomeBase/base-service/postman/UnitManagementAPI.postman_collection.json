									"console.log('- user_id:', pm.environment.get('user_id'));",
									"console.log('- username:', pm.environment.get('username'));",
									"console.log('- tenant_id:', pm.environment.get('tenant_id'));",
									"console.log('========================');",
									"",
									"console.log('Response status:', pm.response.code);",
									"console.log('Response headers:', JSON.stringify(pm.response.headers, null, 2));",
									"console.log('Response body:', pm.response.text());",
									"",
									"if (pm.response.code === 200) {",
									"    try {",
									"        const response = pm.response.json();",
									"        console.log('Parsed response:', response);",
									"",
									"        if (response.token) {",
									"            pm.environment.set('jwt_token', response.token);",
									"            console.log('✅ Token saved successfully!');",
									"            console.log('Token (first 50 chars):', response.token.substring(0, 50) + '...');",
									"            console.log('Token length:', response.token.length);",
									"            console.log('User info:', response.user);",
