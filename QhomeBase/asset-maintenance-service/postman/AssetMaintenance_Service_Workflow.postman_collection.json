		},
		{
			"name": "3. Resident Booking Flow",
			"item": [
				{
					"name": "Get Service Slots (Resident)",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{resident_token}}"
							}
						],
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services/{{service_id}}/slots?fromDate={{current_date}}&toDate={{current_date}}",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"services",
								"{{service_id}}",
								"slots"
							],
							"query": [
								{
									"key": "fromDate",
									"value": "{{current_date}}"
								},
								{
									"key": "toDate",
									"value": "{{current_date}}"
								}
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Service slots loaded\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Get Service Slots By Date (Resident)",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{resident_token}}"
							}
						],
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services/{{service_id}}/slots/{{current_date}}",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"services",
								"{{service_id}}",
								"slots",
								"{{current_date}}"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Service slots for date loaded\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Get Booking Catalog (Resident)",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{resident_token}}"
							}
						],
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services/{{service_id}}/booking/catalog",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"services",
								"{{service_id}}",
								"booking",
								"catalog"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Catalog loaded\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Create Booking (Resident)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{resident_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"serviceId\": \"{{service_id}}\",\n  \"bookingDate\": \"{{current_date}}\",\n  \"startTime\": \"07:00\",\n  \"endTime\": \"08:30\",\n  \"durationHours\": 1.5,\n  \"numberOfPeople\": 1,\n  \"purpose\": \"Morning training session\",\n  \"totalAmount\": 500000,\n  \"termsAccepted\": true,\n  \"items\": [\n    {\n      \"itemType\": \"OPTION\",\n      \"itemId\": \"{{service_option_id}}\",\n      \"itemCode\": \"{{service_option_code}}\",\n      \"itemName\": \"{{service_option_name}}\",\n      \"quantity\": 1,\n      \"unitPrice\": 150000,\n      \"totalPrice\": 150000\n    }\n  ],\n  \"slots\": [\n    {\n      \"slotDate\": \"{{current_date}}\",\n      \"startTime\": \"07:00\",\n      \"endTime\": \"08:30\"\n    }\n  ]\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/bookings",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"bookings"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "prerequest",
							"script": {
								"exec": [
									"const today = new Date();",
									"const isoDate = today.toISOString().substring(0, 10);",
									"pm.environment.set(\"current_date\", isoDate);"
								],
								"type": "text/javascript"
							}
						},
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Booking created\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"let json = pm.response.json();",
									"pm.environment.set(\"service_booking_id\", json.id);",
									"pm.environment.set(\"service_booking_total\", json.totalAmount);"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Get My Bookings (Resident)",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{resident_token}}"
							}
						],
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/bookings",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"bookings"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Bookings returned\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
				}
			]
		}
	],
	"event": [],
	"variable": []
}

