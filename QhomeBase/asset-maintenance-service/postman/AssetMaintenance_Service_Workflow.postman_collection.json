						"method": "PUT",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{admin_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"items\": [\n    {\n      \"optionId\": \"{{service_option_id}}\",\n      \"sortOrder\": 1\n    }\n  ]\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/service-option-groups/{{service_option_group_id}}/items",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"service-option-groups",
								"{{service_option_group_id}}",
								"items"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Option group updated\", function () {",
									"    pm.response.to.have.status(200);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					]
