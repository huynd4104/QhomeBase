							"raw": "{\n  \"code\": \"{{service_option_group_code}}\",\n  \"name\": \"{{service_option_group_name}}\",\n  \"description\": \"Optional add-ons\",\n  \"minSelect\": 0,\n  \"maxSelect\": 2,\n  \"isRequired\": false,\n  \"sortOrder\": 1\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services/{{service_id}}/option-groups",
