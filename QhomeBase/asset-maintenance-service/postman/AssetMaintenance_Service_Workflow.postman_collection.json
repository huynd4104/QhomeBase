						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{admin_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"code\": \"{{service_combo_code}}\",\n  \"name\": \"{{service_combo_name}}\",\n  \"description\": \"Training bundle with add-on\",\n  \"price\": 450000,\n  \"durationMinutes\": 90,\n  \"isActive\": true,\n  \"items\": [\n    {\n      \"itemName\": \"Personal Training Session\",\n      \"itemDescription\": \"1-on-1 PT for 60 minutes\",\n      \"itemPrice\": 350000,\n      \"itemDurationMinutes\": 60,\n      \"quantity\": 1,\n      \"note\": \"Includes warm-up guidance\",\n      \"sortOrder\": 0\n    },\n    {\n      \"itemName\": \"Gym Sanitizing\",\n      \"itemDescription\": \"Deep cleaning equipment after use\",\n      \"itemPrice\": 100000,\n      \"itemDurationMinutes\": 30,\n      \"quantity\": 1,\n      \"sortOrder\": 1\n    }\n  ]\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services/{{service_id}}/combos",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"services",
								"{{service_id}}",
								"combos"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Combo created\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"let json = pm.response.json();",
									"pm.environment.set(\"service_combo_id\", json.id);",
									"pm.environment.set(\"service_combo_code\", json.code);",
									"pm.environment.set(\"service_combo_name\", json.name);"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
