								"key": "Authorization",
								"value": "Bearer {{admin_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"code\": \"{{service_ticket_code}}\",\n  \"name\": \"{{service_ticket_name}}\",\n  \"ticketType\": \"HOURLY\",\n  \"durationHours\": 1,\n  \"price\": 200000,\n  \"maxPeople\": 1,\n  \"description\": \"Single day gym access\",\n  \"isActive\": true,\n  \"sortOrder\": 1\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services/{{service_id}}/tickets",
							"host": [
