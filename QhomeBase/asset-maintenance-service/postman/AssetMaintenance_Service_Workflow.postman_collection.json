{
	"info": {
		"_postman_id": "asset-maintenance-service-workflow",
		"name": "Asset Maintenance Service Workflow",
		"description": "End-to-end workflow to configure services, combos, options, tickets and execute a resident booking in asset-maintenance-service using real IAM login.",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "0. Auth",
			"item": [
				{
					"name": "Login as Admin",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{admin_username}}\",\n  \"password\": \"{{admin_password}}\"\n}"
						},
						"url": {
							"raw": "{{iam_service_url}}/api/auth/login",
							"host": [
								"{{iam_service_url}}"
							],
							"path": [
								"api",
								"auth",
								"login"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Login succeeded\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"let json = pm.response.json();",
									"if (!json || !json.accessToken) {",
									"    throw new Error(\"accessToken not present in login response\");",
									"}",
									"pm.environment.set(\"admin_token\", json.accessToken);"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Login as Resident",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"username\": \"{{resident_username}}\",\n  \"password\": \"{{resident_password}}\"\n}"
						},
						"url": {
							"raw": "{{iam_service_url}}/api/auth/login",
							"host": [
								"{{iam_service_url}}"
							],
							"path": [
								"api",
								"auth",
								"login"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Login succeeded\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"let json = pm.response.json();",
									"if (!json || !json.accessToken) {",
									"    throw new Error(\"accessToken not present in login response\");",
									"}",
									"pm.environment.set(\"resident_token\", json.accessToken);"
								],
								"type": "text/javascript"
							}
						}
					]
				}
			]
		},
		{
			"name": "1. Service Configuration",
			"item": [
				{
					"name": "Create Service Category (Admin)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{admin_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"code\": \"{{service_category_code}}\",\n  \"name\": \"{{service_category_name}}\",\n  \"description\": \"Gym and wellness related services\",\n  \"isActive\": true\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/service-categories",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"service-categories"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Category created\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"let json = pm.response.json();",
									"pm.environment.set(\"service_category_id\", json.id);",
									"pm.environment.set(\"service_category_code\", json.code);",
									"pm.environment.set(\"service_category_name\", json.name);"
								],
								"type": "text/javascript"
							}
						}
					]
				},
				{
					"name": "Create Service (Admin)",
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/json"
							},
							{
								"key": "Authorization",
								"value": "Bearer {{admin_token}}"
							}
						],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"categoryId\": \"{{service_category_id}}\",\n  \"code\": \"{{service_code}}\",\n  \"name\": \"{{service_name}}\",\n  \"description\": \"Personal training sessions for residents\",\n  \"location\": \"Community Gym\",\n  \"pricingType\": \"HOURLY\",\n  \"pricePerHour\": 350000,\n  \"minDurationHours\": 1,\n  \"rules\": \"Arrive 10 minutes early for warm-up\",\n  \"isActive\": true\n}"
						},
						"url": {
							"raw": "{{asset_service_url}}/api/asset-maintenance/services",
							"host": [
								"{{asset_service_url}}"
							],
							"path": [
								"api",
								"asset-maintenance",
								"services"
							]
						}
					},
					"response": [],
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Service created\", function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"let json = pm.response.json();",
									"pm.environment.set(\"service_id\", json.id);",
									"pm.environment.set(\"service_code\", json.code);",
									"pm.environment.set(\"service_name\", json.name);"
								],
								"type": "text/javascript"
							}
						}
					]
				}
			]
		},
